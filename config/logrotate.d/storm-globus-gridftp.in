#
# Log rotation of GridFTP logs
#

@localstatedir@/log/@PACKAGE_PREFIX_NAME@/storm-globus-gridftp.log {
    compress
    daily
    delaycompress
    missingok
    rotate 90
}

@localstatedir@/log/@PACKAGE_PREFIX_NAME@/storm-gridftp-session.log {
    compress
    daily
    delaycompress
    missingok
    rotate 90
}
